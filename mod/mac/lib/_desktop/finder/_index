# shellcheck shell=dash

xrc:mod:lib     mac     _desktop/finder/hidden
___x_cmd_mac_finder(){
    local op="$1";  shift
    case "$op" in
        hidden|desktop|path|extension)
            ___x_cmd_mac_finder_"$op" "$@"
            ;;
    esac
}

___x_cmd_mac_finder_hidden(){
    ___x_cmd_mac_hidden "$@"
}

___x_cmd_mac_finder_desktop(){
    :
}
